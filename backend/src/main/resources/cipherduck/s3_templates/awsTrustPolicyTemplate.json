{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "{oidcProvider}"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "ForAnyValue:StringLike": {
          "login1.staging.cryptomator.cloud/realms/cipherduck:amr": [
            "{vaultId}"
          ]
        }
      }
    }
  ]
}