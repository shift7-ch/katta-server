{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::930717317329:role/cipherduck_chain_01"
      },
      "Action": [
        "sts:AssumeRole",
        "sts:TagSession"
      ],
      "Condition": {
        "ForAnyValue:StringEquals": {
          "sts:TransitiveTagKeys": "${aws:RequestTag/VaultR}"
        }
      }
    }
  ]
}